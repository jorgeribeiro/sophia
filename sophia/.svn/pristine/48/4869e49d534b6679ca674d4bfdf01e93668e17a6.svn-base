@charset "UTF-8";

/* Estilos da interface anterior */
.window-div {
	margin-top: 1%;
	border: 1px solid #E1E1E1;
	padding: 20px 20px;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
}

.z-notification-content {
	display: table-cell;
	width: 200px;
	height: 100px;
	min-height: 60px;
	padding: 5px 18px 5px 45px;
	vertical-align: middle;
	font-size: 13px;
}

.z-panel {
	background-image: none;
	position: absolute;
	margin-left: auto;
	margin-right: auto;
	left: 0;
	right: 0;
	margin-top: 2%;
	box-shadow: 1px 2px 6px rgba(0, 0, 0, 0.5);
	-moz-box-shadow: 1px 2px 6px rgba(0, 0, 0, 0.5);
	-webkit-box-shadow: 1px 2px 6px rgba(0, 0, 0, 0.5);
	background-color: #ffffff;
}

.z-menubar {
	padding: 5px 0;
	background: #8B0000;
	margin: 0 auto;
	width: 100%;
	position: relative;
	vertical-align: baseline;
	border: none;
	text-align: center;
}

.z-menu-text, .z-menuitem-text {
	font-size: 13px;
	text-shadow: none;
	color: #E3E3E3;
	font-weight: bold;
}

.z-menu-icon, .z-menu-icon:hover, .z-menu-icon:active {
	color: #E3E3E3;
}

.z-menu-selected>.z-menu-content, .z-menu-content:hover,
	.z-menuitem-content:hover, .z-menu-content:focus, .z-menuitem-content:focus
	{
	box-shadow: none;
	background: #550000;
	border-color: #550000;
}

.z-menu-hover>.z-menu-content, .z-menuitem-hover>.z-menuitem-content {
	box-shadow: none;
	background: #550000;
	border-color: #550000;
}

.z-menupopup .z-menuitem-image {
	min-width: 0;
	min-height: 0;
	margin-right: 0;
}

.z-menupopup-separator {
	width: 0;
	height: 0;
	border: none;
	background: transparent;
}

.z-menupopup {
	border: none;
	background: #8B0000;
}

.z-menubar ul {
	display: flex;
	justify-content: center;
	width: 100%;
	margin: 0;
	padding: 0;
}

.z-menubar-horizontal li {
	margin: 0 5px;
	float: left;
}

.z-toolbarbutton {
	display: flex;
	justify-content: center;
}

.z-combobox-popup, z-combobox-open, z-combobox-shadow {
	height: 200px;
}

/* O que é isso aqui abaixo? Inserção de Francisco */

.title {
	font-weight: bold;
	font-size: 12px;
}

.demoTbx .z-tab {
	margin-right: 2px;
}